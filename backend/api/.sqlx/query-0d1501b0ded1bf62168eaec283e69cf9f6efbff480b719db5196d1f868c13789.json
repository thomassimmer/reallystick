  "query": "\n            SELECT DISTINCT\n                c.id,\n                c.name,\n                c.description,\n                c.icon,\n                c.created_at,\n                c.start_date,\n                c.creator,\n                c.deleted\n            FROM challenges c\n            LEFT JOIN challenge_participations cp ON c.id = cp.challenge_id\n            WHERE cp.user_id = $1 OR c.creator = $1\n            ",
