  "query": "\n            SELECT DISTINCT\n                h.id,\n                h.name,\n                h.category_id,\n                h.reviewed,\n                h.description,\n                h.icon,\n                h.created_at,\n                h.unit_ids\n            FROM habits h\n            LEFT JOIN habit_participations hp ON h.id = hp.habit_id\n            WHERE h.reviewed = true OR hp.user_id = $1\n            ",
